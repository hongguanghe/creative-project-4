AAA,EAAA;AACA,IAAA,UADA,sBACA,IADA,EACA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,eAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,WAAA,UAAA,GAAA,EAAA;AACA,KANA;AAOA,IAAA,WAPA,uBAOA,KAPA,EAOA;AACA,WAAA,IAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,KATA;AAUA,IAAA,MAVA,oBAUA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,QAFA,GAEA,IAAA,QAAA,EAFA;AAGA,gBAAA,QAAA,CAAA,MAAA,CAAA,OAAA,EAAA,MAAA,CAAA,IAAA,EAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AAHA;AAAA,uBAIA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA,QAAA,CAJA;;AAAA;AAIA,gBAAA,EAJA;AAAA;AAAA,uBAKA,KAAA,CAAA,IAAA,CAAA,YAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,KADA;AAEA,kBAAA,MAAA,EAAA,MAAA,CAAA,MAFA;AAGA,kBAAA,WAAA,EAAA,MAAA,CAAA,WAHA;AAIA,kBAAA,IAAA,EAAA,EAAA,CAAA,IAAA,CAAA;AAJA,iBAAA,CALA;;AAAA;AAKA,gBAAA,EALA;AAWA,gBAAA,MAAA,CAAA,OAAA,GAAA,EAAA,CAAA,IAAA;AAXA;AAAA;;AAAA;AAAA;AAAA;AAAA,iDAaA,OAbA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA,KAzBA;AA0BA,IAAA,UA1BA,sBA0BA,IA1BA,EA0BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAJA,kDAKA,IALA;;AAAA;AAAA;AAAA;AAAA,kDAOA,OAPA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA,KApCA;AAqCA,IAAA,QArCA,oBAqCA,IArCA,EAqCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KADA;AAEA,kBAAA,MAAA,EAAA,MAAA,CAAA,QAAA,CAAA,MAFA;AAGA,kBAAA,WAAA,EAAA,MAAA,CAAA,QAAA,CAAA;AAHA,iBAAA,CAFA;;AAAA;AAOA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AARA,kDASA,IATA;;AAAA;AAAA;AAAA;AAAA,kDAWA,OAXA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA,KAnDA;AAoDA,IAAA,QApDA,sBAoDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,YAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAAA,kDAMA,OANA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA;AA7DA;AA5BA,CAAA","sourcesContent":["<template>\r\n<div class=\"allContent\">\r\n  <h1>Post and Manage Comments</h1>\r\n  <div class=\"info\">\r\n    <div class=\"first\">\r\n      <div class=\"heading\">\r\n        <div class=\"circle\">1</div>\r\n        <h2>Post a Comment</h2>\r\n      </div>\r\n      <div class=\"add\">\r\n        <div class=\"form\">\r\n          <input v-model=\"title\" placeholder=\"Title\">\r\n          <p></p>\r\n          <input v-model=\"author\" placeholder=\"Your Name\">\r\n          <p></p>\r\n          <textarea v-model=\"description\" name=\"name\" rows=\"4\" cols=\"40\" placeholder=\"Enter Your Comment\"></textarea>\r\n          <p></p>\r\n          <input type=\"file\" name=\"photo\" @change=\"fileChanged\">\r\n          <button @click=\"upload\" id=\"submit-button\">Submit</button>\r\n        </div>\r\n        <div class=\"upload\" v-if=\"addItem\">\r\n          <h2>{{addItem.title}}</h2>\r\n          <h5>{{addItem.description}}</h5>\r\n          <img :src=\"addItem.path\" />\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"second\">\r\n      <div class=\"heading\">\r\n      <div class=\"circle\">2</div>\r\n      <h2>Edit/Delete an Item</h2>\r\n      </div>\r\n      <div class=\"edit\">\r\n        <div class=\"form\">\r\n          <input v-model=\"findTitle\" placeholder=\"Search by Author\" id=\"searchBox\">\r\n          <div class=\"suggestions\" v-if=\"suggestions.length > 0\">\r\n            <div class=\"suggestion\" v-for=\"s in suggestions\" :key=\"s.id\" @click=\"selectItem(s)\">{{s.title}}\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"edit-elements\" v-if=\"findItem\">\r\n          <div class=\"col1\">\r\n            <input v-model=\"findItem.title\" placeholder=\"Edited Title\">\r\n            <p></p>\r\n            <input v-model=\"author\" placeholder=\"Edited Name\">\r\n            <p></p>\r\n            <textarea name=\"name\" rows=\"4\" cols=\"40\" placeholder=\"Edited Comment\" v-model=\"findItem.description\"></textarea>\r\n            <div class=\"col2\">\r\n              <img :src=\"findItem.path\" />\r\n            </div>\r\n            <div class=\"actions\" v-if=\"findItem\">\r\n              <button @click=\"editItem(findItem)\" id=\"editButton\">Edit</button>\r\n              <button @click=\"deleteItem(findItem)\">Delete</button>\r\n            </div>\r\n          </div>\r\n\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</div>\r\n\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'Admin',\r\n  data() {\r\n    return {\r\n      items: [],\r\n      author:\"\",\r\n      title: \"\",\r\n      description:\"\",\r\n      file: null,\r\n      addItem: null,\r\n      selected: false,\r\n      findItem: null,\r\n      findTitle: \"\",\r\n      findAuthor:\"\",\r\n      findDescription: \"\",\r\n      editedTitle:\"\",\r\n      editedAuthor:\"\",\r\n    }\r\n  },\r\n  created() {\r\n    this.getItems();\r\n  },\r\n  computed: {\r\n    suggestions() {\r\n      let items = this.items.filter(item => item.title.toLowerCase().startsWith(this.findTitle.toLowerCase()));\r\n      return items.sort((a, b) => a.title > b.title);\r\n    }\r\n  },\r\n  methods: {\r\n    selectItem(item) {\r\n      this.findTitle = \"\";\r\n      this.findDescription = \"\";\r\n      this.findItem = item;\r\n      this.findAuthor = \"\";\r\n    },\r\n    fileChanged(event) {\r\n      this.file = event.target.files[0];\r\n    },\r\n    async upload() {\r\n      try {\r\n        const formData = new FormData();\r\n        formData.append('photo', this.file, this.file.name)\r\n        let r1 = await axios.post('/api/photos', formData);\r\n        let r2 = await axios.post('/api/items', {\r\n          title: this.title,\r\n          author:this.author,\r\n          description: this.description,\r\n          path: r1.data.path\r\n        });\r\n        this.addItem = r2.data;\r\n      } catch (error) {\r\n        return \"Error\";\r\n      }\r\n    },\r\n    async deleteItem(item) {\r\n      try {\r\n        await axios.delete(\"/api/items/\" + item._id);\r\n        this.findItem = null;\r\n        this.getItems();\r\n        return true;\r\n      } catch (error) {\r\n        return \"Error\";\r\n\r\n      }\r\n    },\r\n    async editItem(item) {\r\n      try {\r\n        await axios.put(\"/api/items/\" + item._id, {\r\n          title: this.findItem.title,\r\n          author: this.findItem.author,\r\n          description: this.findItem.description,\r\n        });\r\n        this.findItem = null;\r\n        this.getItems();\r\n        return true;\r\n      } catch (error) {\r\n        return \"Error\";\r\n\r\n      }\r\n    },\r\n    async getItems() {\r\n      try {\r\n        let response = await axios.get(\"/api/items\");\r\n        this.items = response.data;\r\n        return true;\r\n      } catch (error) {\r\n        return \"Error\";\r\n\r\n      }\r\n    },\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.image h2 {\r\n  font-style: italic;\r\n  font-size: 1em;\r\n}\r\n\r\n.heading {\r\n  display: flex;\r\n  margin-bottom: 20px;\r\n  margin-top: 20px;\r\n}\r\n\r\n.heading h2 {\r\n  margin-top: 8px;\r\n  margin-left: 10px;\r\n}\r\n\r\n.add,\r\n.edit {\r\n  display: flex;\r\n}\r\n\r\n.circle {\r\n  border-radius: 50%;\r\n  width: 40px;\r\n  height: 40px;\r\n  padding: 8px;\r\n  background:#ffbb00;\r\n  color: #fff;\r\n  text-align: center\r\n}\r\n\r\n/* Form */\r\ninput,\r\ntextarea,\r\nselect,\r\nbutton {\r\n  font-family: 'Montserrat', sans-serif;\r\n  font-size: 1em;\r\n}\r\n\r\n.form {\r\n  margin-right: 50px;\r\n}\r\n\r\n/* Uploaded images */\r\n.upload h2 {\r\n  margin: 0px;\r\n}\r\n\r\n.upload img {\r\n  max-width: 300px;\r\n}\r\n\r\n.itemLists{\r\n  display: flex;\r\n  flex-direction: row;\r\n}\r\n\r\n.col1{\r\n  margin-right: 20px;\r\n}\r\n\r\n.col2 img{\r\n  max-width: 300px;\r\n}\r\n\r\n.edit-elements{\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-space-between;\r\n}\r\n\r\n.actions{\r\n  margin-top: 30px;\r\n}\r\n\r\n#editButton{\r\n  margin-right: 10px;\r\n}\r\n\r\n.info{\r\n  display: flex;\r\n  flex-direction: row;\r\n}\r\n\r\n#searchBox{\r\n  margin-bottom: 10px;\r\n}\r\n\r\n#submit-button{\r\n  margin-top: 10px;\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":"src/views"}]}